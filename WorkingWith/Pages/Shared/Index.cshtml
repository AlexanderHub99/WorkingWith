@model System.Collections.Generic.IEnumerable<WorkingWith.Models.Product>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Worling with Visual Studio</title>
</head>
<body>
<table>
    <thead>
    <tr><td>Name</td><td>Price</td></tr>
    </thead>
    <tbody>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Price</td>
        </tr>
    }
    </tbody>
</table>
</body>
</html>